<% include partials/header.ejs %>
<div class="containerback container">


    <div class="row">

        <div class="courses col-lg-6 col-md-12">
            <h3 class="section">Courses</h3>
            <button class="addbutton btn btn-default btn-primary">Add a subject</button>
            <hr style="margin-top: 0px">

            <% subjects.forEach(function(subject){ %>
            <div class="col-lg-12">
                <div class="col-lg-4">
                    <a href="/<%= subject.name %>">
                        <div class="thumbnail">
                            <img class="mycirimg" src="<%= subject.image %>">
                        </div>
                    </a>
                </div>
                <div class="intro col-lg-8">
                    <a href="/<%= subject.name %>">
                        <h4 id="subject"><%= subject.name.toUpperCase() %></h4>
                        <p id="quote"><%= subject.quote %></p>
                        <p id="author"><%= subject.author %></p>
                    </a>
                </div>
                <hr class="col-lg-11" style="border-top:1px dotted black; align:center;">
            </div>
            <% }) %>

            <div class="col-lg-12">
                <div class="col-lg-4">
                    <div class="thumbnail">
                        <img class="cirimg" src="comingsoon.jpg">
                    </div>
                </div>
                <div class="col-lg-8">
                    <div>
                        <h4 id="subject">COMING SOON</h4>
                    </div>
                </div>
            </div>
            <hr class="col-lg-11" style="border-top:1px dotted black; align:center; margin-bottom: 0px;">
        </div>


        <div class="reviews col-lg-4 col-md-6">
            <h3 class="section">Reviews</h3>
            <button class="addbutton btn btn-default btn-primary">Add a review</button>
            <hr style="margin-top: 0px">
        </div>



        <div class="Links col-lg-2 col-md-12">

            <h3 class="section" style="padding-bottom:6px;">Links</h3>
            <hr style="margin-top: 0px">

            <a href="https://www.youtube.com/channel/UC45PmK55T7J_G6-ejkFO4hw">
                <div class="thumbnail">
                    <img class="banner" src="youtubebanner.jpg">
                </div>
            </a>

            <a href="https://github.com/MTLChrisLEE">
            <div class="thumbnail">
                <img class="banner" src="githubbanner.jpg">
            </div>
            </a>

            <a href="https://stackoverflow.com/">
            <div class="thumbnail">
                <img class="banner" src="stackoverflowbanner.jpg">
            </div>
            </a>

            <a href="https://techcrunch.com/">
                <div class="thumbnail">
                    <img class="banner" src="TechCrunchbanner.jpg">
                </div>
            </a>
        </div>

    </div>

    <% include partials/footer.ejs %>



